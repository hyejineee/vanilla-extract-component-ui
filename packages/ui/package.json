		"react": ">=18.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@vanilla-extract/css": "^1.14.2",
		"@vanilla-extract/vite-plugin": "^4.0.7",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"publint": "^0.2.7",
		"react": "^18.2.0",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	}
